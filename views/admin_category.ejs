<head>
    <style>

        table {
            border-collapse: collapse;
        }

        table, th, td {
            border : 1px solid black;
            padding: 5px
        }

    </style>
</head>

<body>
<% include navbaradmin.ejs %>
    <h1>list kategori</h1>

    <table>
            <tr>
                <th>Season</th>
                <th>Nama kategori</th>
                <th colspan = 3></th>
            </tr>
            <% kategori_produk.forEach(x => { %>
                <tr>
                    <td><%= x.season%></td>
                    <td><%= x.kategori_produk%></td>
                    <td><a href="/admin/admin_product/<%= x.id%>"> lihat produk kategori </a></td>
                    <td><a href="/admin/admin_category_edit/<%= x.id%>"> edit kategori </a></td>
                    <td><a href="/admin/admin_category_delete/<%= x.id%>"> delete kategori </a></td>
                </tr>
            <% }) %>
        </table>
        <br/>
    <p><a href="/admin/admin_category_register/<%=id%>"> tambah kategori </a><p>

    <p><a href="/admin/adminhome"> balik ke rumah </a></p>
    <!--<p><button class='btn btn primary' type='button' onclick='javascript:history.back()'>Balik</button></p>-->

</body>
